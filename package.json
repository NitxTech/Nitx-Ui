        "@types/react-dom": "^19.1.6",
        "autoprefixer": "^10.4.21",
        "class-variance-authority": "^0.7.1",
        "dotenv": "^17.2.0",
        "lucide-react": "^0.525.0",
        "next": "^15.3.5",
        "postcss": "^8.5.6",
        "storybook": "^8.6.14",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^3.4.17",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "webpack": "^5.100.0"
    },
    "publishConfig": {
        "access": "public"
    }
}