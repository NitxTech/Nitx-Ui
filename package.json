{
    "name": "nitxui",
    "version": "1.0.2",
    "private": false,
    "description": "The official UI component library for Nitx projects.",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
